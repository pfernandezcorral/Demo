<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <validationRules>
        <fullName>copado__Environment_Required</fullName>
        <active>true</active>
        <description>If the Type value is equal to &quot;Regression Test&quot; or &quot;User Story Test&quot;, the Environment field must have a value.</description>
        <errorConditionFormula>AND( ISBLANK( copado__Environment__c ), OR(ISPICKVAL( copado__Type__c , &quot;Regression Test&quot;), ISPICKVAL( copado__Type__c , &quot;User Story Test&quot;)))</errorConditionFormula>
        <errorDisplayField>copado__Environment__c</errorDisplayField>
        <errorMessage>The Environment field is required if the Type is &quot;User Story Test&quot; or &quot;Regression Test&quot;.</errorMessage>
    </validationRules>
    <webLinks>
        <fullName>copado__Refresh_Results</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Refresh Results</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>document.location.reload(true);</url>
    </webLinks>
    <webLinks>
        <fullName>copado__Run_Group</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>page</linkType>
        <masterLabel>Run Group</masterLabel>
        <openType>replace</openType>
        <page>copado__SeleniumRunGroup</page>
        <protected>false</protected>
    </webLinks>
    <webLinks>
        <fullName>copado__Show_Hook_URL</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Show Hook URL</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>showWebhook();</url>
    </webLinks>
</CustomObject>
